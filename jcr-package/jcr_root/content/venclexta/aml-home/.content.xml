<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        jcr:primaryType="cq:PageContent"
        jcr:title="AML Home"
        jcr:description="AML treatment information page"
        sling:resourceType="core/wcm/components/page/v3/page"
        cq:template="/conf/venclexta/settings/wcm/templates/pharma-detail"
        cq:lastModified="{Date}2025-01-07T12:00:00.000Z"
        pageTitle="Venclexta AML Home">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="core/wcm/components/container/v1/container">
            <hero
                jcr:primaryType="nt:unstructured"
                sling:resourceType="core/wcm/components/teaser/v2/teaser"
                jcr:title="Treatment Information"
                jcr:description="Clinical study results and treatment details"
                fileReference="/content/dam/venclexta/aml-home/hero-image.png"
                titleFromPage="false"
                actionsEnabled="true">
                <actions jcr:primaryType="nt:unstructured">
                    <item0
                        jcr:primaryType="nt:unstructured"
                        title="Study Information"
                        link="/content/venclexta/study-design"/>
                    <item1
                        jcr:primaryType="nt:unstructured"
                        title="Clinical Guidelines"
                        link="/content/venclexta/guidelines"/>
                </actions>
            </hero>
            <cards
                jcr:primaryType="nt:unstructured"
                sling:resourceType="core/wcm/components/container/v1/container">
                <card1
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="core/wcm/components/teaser/v2/teaser"
                    jcr:title="Overall Survival"
                    link="/content/venclexta/aml-efficacy"/>
                <card2
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="core/wcm/components/teaser/v2/teaser"
                    jcr:title="Remission Data"
                    link="/content/venclexta/aml-efficacy#remission"/>
                <card3
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="core/wcm/components/teaser/v2/teaser"
                    jcr:title="Independence Metrics"
                    link="/content/venclexta/aml-efficacy#metrics"/>
                <card4
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="core/wcm/components/teaser/v2/teaser"
                    jcr:title="Assessment Information"
                    link="/content/venclexta/aml-efficacy#assessment"/>
            </cards>
            <abbreviations
                jcr:primaryType="nt:unstructured"
                sling:resourceType="core/wcm/components/text/v2/text"
                text="&lt;p&gt;Medical abbreviations and definitions section&lt;/p&gt;"/>
            <isi_fragment
                jcr:primaryType="nt:unstructured"
                sling:resourceType="core/wcm/components/experiencefragment/v2/experiencefragment"
                fragmentVariationPath="/content/experience-fragments/venclexta/aml/isi/master"/>
        </root>
    </jcr:content>
</jcr:root>
